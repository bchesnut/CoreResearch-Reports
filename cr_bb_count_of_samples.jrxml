	<parameter name="SAPPHIRE_START_DATE" class="java.util.Date"/>
	<parameter name="SAPPHIRE_END_DATE" class="java.util.Date"/>
	<parameter name="STUDY_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
       study.s_studyid,
       study.studydesc,
       d.departmentid,
       d.departmentdesc,
       sysuser.sysuserdesc,
       s.sampletypeid, 
       count(*)
  from s_samplefamily sf
  join s_sample s on s.samplefamilyid = sf.s_samplefamilyid
  join s_study study on study.s_studyid=sf.SSTUDYID
   left outer join s_participant p on p.s_participantid = sf.participantid
  join department d on d.departmentid = study.securitydepartment
  join departmentsysuser dsu on dsu.departmentid = d.departmentid
  join sysuser sysuser on sysuser.sysuserid = dsu.sysuserid
 where 
   instr($P{STUDY_ID} || ';', study.s_studyid || ';') > 0
   and s.receiveddt between  $P{SAPPHIRE_START_DATE} and  $P{SAPPHIRE_END_DATE} 
   and  dsu.sysuserid= $P{SAPPHIRE_CurrentUser} 
     
 group by study.s_studyid,
       study.studydesc,
       d.departmentid,
       d.departmentdesc,
       sysuser.sysuserdesc,
       s.sampletypeid
 order by study.s_studyid, s.sampletypeid]]>
	</queryString>
